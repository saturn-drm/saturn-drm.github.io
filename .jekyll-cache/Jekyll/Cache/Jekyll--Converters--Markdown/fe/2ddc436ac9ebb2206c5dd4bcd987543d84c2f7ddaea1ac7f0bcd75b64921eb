<h3 id="duration--function-based-parameters">Duration + Function Based Parameters</h3>

<ul>
  <li>Javascript</li>
</ul>

<div class="language-js highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="o">&lt;</span><span class="nx">script</span> <span class="kd">class</span><span class="o">=</span><span class="dl">"</span><span class="s2">anime</span><span class="dl">"</span><span class="o">&gt;</span>
  <span class="nx">anime</span><span class="p">({</span>
    <span class="na">targets</span><span class="p">:</span> <span class="dl">'</span><span class="s1">.duration</span><span class="dl">'</span><span class="p">,</span>
    <span class="na">translateX</span><span class="p">:</span> <span class="dl">'</span><span class="s1">60vw</span><span class="dl">'</span><span class="p">,</span>
    <span class="na">duration</span><span class="p">:</span> <span class="kd">function</span><span class="p">(</span><span class="nx">el</span><span class="p">,</span> <span class="nx">i</span><span class="p">){</span>
      <span class="k">return</span> <span class="p">(</span><span class="nx">i</span> <span class="o">+</span> <span class="mi">1</span><span class="p">)</span> <span class="o">*</span> <span class="mi">1000</span><span class="p">;</span>
    <span class="p">},</span>
    <span class="na">loop</span><span class="p">:</span> <span class="kc">true</span>
  <span class="p">});</span>
<span class="o">&lt;</span><span class="sr">/script</span><span class="err">&gt;
</span></code></pre></div></div>

<h3 id="delay--function-based-parameters">Delay + Function Based Parameters</h3>

<ul>
  <li>Javascript</li>
</ul>

<div class="language-js highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="o">&lt;</span><span class="nx">script</span> <span class="kd">class</span><span class="o">=</span><span class="dl">"</span><span class="s2">anime</span><span class="dl">"</span><span class="o">&gt;</span>
  <span class="nx">anime</span><span class="p">({</span>
    <span class="na">targets</span><span class="p">:</span> <span class="dl">'</span><span class="s1">.delay</span><span class="dl">'</span><span class="p">,</span>
    <span class="na">translateX</span><span class="p">:</span> <span class="dl">'</span><span class="s1">60vw</span><span class="dl">'</span><span class="p">,</span>
    <span class="na">delay</span><span class="p">:</span> <span class="kd">function</span><span class="p">(</span><span class="nx">el</span><span class="p">,</span> <span class="nx">i</span><span class="p">){</span>
      <span class="k">return</span> <span class="p">(</span><span class="nx">i</span> <span class="o">+</span> <span class="mi">1</span><span class="p">)</span> <span class="o">*</span> <span class="mi">1000</span><span class="p">;</span>
    <span class="p">},</span>
    <span class="na">loop</span><span class="p">:</span> <span class="kc">true</span>
  <span class="p">});</span>
<span class="o">&lt;</span><span class="sr">/script</span><span class="err">&gt;
</span></code></pre></div></div>

<h3 id="end-delay">End Delay</h3>

<ul>
  <li>Javascript</li>
</ul>

<div class="language-js highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="o">&lt;</span><span class="nx">script</span> <span class="kd">class</span><span class="o">=</span><span class="dl">"</span><span class="s2">anime</span><span class="dl">"</span><span class="o">&gt;</span>
  <span class="nx">animationa</span> <span class="o">=</span> <span class="nx">anime</span><span class="p">({</span>
    <span class="na">targets</span><span class="p">:</span> <span class="nb">document</span><span class="p">.</span><span class="nx">querySelector</span><span class="p">(</span><span class="dl">'</span><span class="s1">#enddelaya</span><span class="dl">'</span><span class="p">),</span>
    <span class="na">translateX</span><span class="p">:</span> <span class="dl">'</span><span class="s1">60vw</span><span class="dl">'</span><span class="p">,</span>
    <span class="na">endDelay</span><span class="p">:</span> <span class="mi">1000</span><span class="p">,</span>
    <span class="na">direction</span><span class="p">:</span> <span class="dl">'</span><span class="s1">alternate</span><span class="dl">'</span><span class="p">,</span>
    <span class="na">loop</span><span class="p">:</span> <span class="kc">true</span>
  <span class="p">});</span>
  <span class="nx">animationb</span> <span class="o">=</span> <span class="nx">anime</span><span class="p">({</span>
    <span class="na">targets</span><span class="p">:</span> <span class="nb">document</span><span class="p">.</span><span class="nx">querySelector</span><span class="p">(</span><span class="dl">'</span><span class="s1">#enddelayb</span><span class="dl">'</span><span class="p">),</span>
    <span class="na">translateX</span><span class="p">:</span> <span class="dl">'</span><span class="s1">60vw</span><span class="dl">'</span><span class="p">,</span>
    <span class="na">endDelay</span><span class="p">:</span> <span class="mi">2000</span><span class="p">,</span>
    <span class="na">direction</span><span class="p">:</span> <span class="dl">'</span><span class="s1">alternate</span><span class="dl">'</span><span class="p">,</span>
    <span class="na">loop</span><span class="p">:</span> <span class="kc">true</span>
  <span class="p">});</span>
<span class="o">&lt;</span><span class="sr">/script</span><span class="err">&gt;
</span></code></pre></div></div>

